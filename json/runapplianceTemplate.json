{
  "template": "run*",

  "settings": {
    "analysis": {
      "analyzer": {
        "prefix-test-analyzer": {
          "type": "custom",
          "tokenizer": "prefix-test-tokenizer"
        }
      },
      "tokenizer": {
        "prefix-test-tokenizer": {
          "type": "path_hierarchy",
          "delimiter": " "
        }
      }
    },
    "index": {
      "number_of_shards": 2,
      "number_of_replicas": 1,
      "codec": "best_compression"
    }
  },
  "mappings": {
    "prc-i-state": {
      "_all": {"enabled": "false" },
      "properties": {
        "macro": {
          "type": "integer"
        },
        "mini": {
          "type": "integer"
        },
        "micro": {
          "type": "integer"
        },
        "tp": {
          "type": "double"
        },
        "lead": {
          "type": "double"
        },
        "nfiles": {
          "type": "integer"
        },
        "lockwaitUs": {
          "type": "double"
        },
        "lockcount": {
          "type": "integer"
        },
        "fm_date": {
          "type": "date"
        },
        "source": {
          "type": "string",
          "index":"not_analyzed"
        },
        "mclass" : {
          "type": "string",
          "index":"not_analyzed"
        }
      },
      "_ttl": {
        "enabled": true,
        "default": 300000
      }
    },
    "prc-s-state": {
      "_timestamp": {
        "enabled": false
      },
      "_all": {"enabled": "false" },
      "_source": {"enabled": false },
      "properties": {
        "macro_s": {
          "type": "integer",
          "index": "no",
          "store": true,
          "doc_values":true
        },
        "miniv": {
          "properties":{
            "key":{
              "type":"integer",
              "index":"no",
              "doc_values":false,
              "store":true
            },
            "value":{
              "type":"integer",
              "index":"no",
              "doc_values":false,
              "store":true
            }
          }
        },
        "microv": {
          "properties":{
            "key":{
              "type":"integer",
              "index":"no",
              "doc_values":false,
              "store":true
            },
            "value":{
              "type":"integer",
              "index":"no",
              "doc_values":false,
              "store":true
            }
          }
        },
        "inputStats" : {
          "properties" : {
            "tp": {
              "type": "double",
              "index":"no",
              "doc_values":true,
              "store": true
            },
            "lead": {
              "type": "double",
              "index":"no",
              "doc_values":true,
              "store": true
            },
            "nfiles": {
              "type": "integer",
              "index":"no",
              "doc_values":true,
              "store": true
            },
            "lockwaitUs": {
              "type": "double",
              "index":"no",
              "doc_values":true,
              "store": true
            },
            "lockcount": {
              "type": "integer",
              "index":"no",
              "doc_values":true,
              "store": true
            }
          }
        },
        "ls": {
          "type": "integer",
          "doc_values":true,
          "store": true
        },
        "fm_date_s": {
          "type": "date",
          "doc_values":false,
          "store": true
        },
        "source_s" : {
          "type":"string",
          "index":"not_analyzed",
          "store": true,
          "doc_values":true
 
        },
        "mclass_s" : {
          "type": "string",
          "index":"not_analyzed",
          "store": true,
          "doc_values":true
        }
      }
    },
    "prc-out": {
      "_all": {"enabled": "false" },
      "_source": {"enabled": false },
      "properties": {
        "din": {
          "type": "integer",
          "index":"no",
          "store":true,
          "doc_values":true
        },
        "dout": {
          "type": "integer",
          "index":"no",
          "store":true,
          "doc_values":true
        },
        "lsn": {
          "type": "integer",
          "store": false,
          "doc_values":false
        },
        "streamn": {
          "type": "string",
          "index": "not_analyzed",
          "store": false,
          "doc_values":false
        },
        "pidn": {
          "type": "integer",
          "store": false,
          "doc_values":false
        },
        "hostn": {
          "type": "string",
          "index": "not_analyzed",
          "store": false,
          "doc_values":false
        },
        "appn": {
          "type": "string",
          "index": "not_analyzed",
          "store": false,
          "doc_values":false
        }
      },
      "_timestamp": {
        "enabled": true
      }
    },
    "prc-in": {
      "_all": {"enabled": "false" },
      "properties": {
        "data": {
          "properties": {
            "out": {
              "type": "integer"
            },
            "size": {
              "type" : "long"
            }
          }
        },
        "ls": {
          "type": "integer",
          "store": true
        },
        "index": {
          "type": "integer"
        },
        "pid": {
          "type": "integer"
        },
        "host": {
          "type": "string",
          "index":"not_analyzed"
        },
        "appliance": {
          "type": "string",
          "index": "not_analyzed"
        },
        "fm_date": {
          "type": "date"
        }
      },
      "_timestamp": {
        "enabled": true
      }
    },
    "fu-out": {
      "_all": {"enabled": "false" },
      "properties": {
        "data": {
          "properties": {
            "in": {
              "type": "integer"
            },
            "out": {
              "type": "integer"
            },
            "errorEvents": {
              "type": "integer"
            },
            "hltErrorEvents": {
              "type": "integer"
            },
            "returnCodeMask": {
              "type": "string",
              "index": "not_analyzed"
            },
            "fileSize": {
              "type": "long"
            },
            "fileAdler32": {
              "type": "long"
            },
            "TransferDestination": {
              "type": "string",
              "index": "not_analyzed"
            },
            "MergeType": {
              "type": "string",
              "index": "not_analyzed"
            },
            "InputFiles": {
              "type": "string",
              "index": "not_analyzed"
            }
          }
        },
        "ls": {
          "type": "integer",
          "store": true
        },
        "stream": {
          "type": "string",
          "index": "not_analyzed"
        },
        "host": {
          "type": "string",
          "index":"not_analyzed"
        },
        "appliance": {
          "type": "string",
          "index": "not_analyzed"
        },
        "fm_date": {
          "type": "date"
        }
      },
      "_timestamp": {
        "enabled": true
      }
    },
    "fu-complete": {
      "_all": {"enabled": "false" },
      "properties": {
        "host": {
          "type": "string",
          "index": "not_analyzed"
        },
        "fm_date": {
          "type": "date"
        }
      },
      "_timestamp": {
        "enabled": true
      }
    },
    "qstatus": {
      "_all": {"enabled": "false" },
      "properties": {
        "numQueuedLS": {
          "type": "integer"
        },
        "maxQueuedLS": {
          "type": "integer"
        },
        "numReadFromQueueLS": {
          "type": "integer"
        },
        "maxClosedLS": {
          "type": "integer"
        },
        "numReadOpenLS": {
          "type": "integer"
        },
        "CMSSWMaxLS": {
          "type": "integer"
        },
        "fm_date": {
          "type": "date"
        },
        "host": {
          "type": "string",
          "index":"not_analyzed"
        }
      },
      "_timestamp": {
        "enabled": true
      }
    },
    "cmsswlog": {
      "_timestamp": {
        "enabled": true
      },
      "_all": {"enabled": "false" },
      "_ttl": {
        "enabled": true,
        "default": 2592000000
      },
      "properties": {
        "host": {
          "type": "string",
          "index": "not_analyzed"
        },
        "pid": {
          "type": "integer"
        },
        "doctype": {
          "type": "string",
          "index": "not_analyzed"
        },
        "severity": {
          "type": "string",
          "index": "not_analyzed"
        },
        "severityVal": {
          "type": "integer"
        },
        "category": {
          "type": "string",
          "index": "not_analyzed"
        },
        "fwkState": {
          "type": "string",
          "index": "not_analyzed"
        },
        "module": {
          "type": "string",
          "index": "not_analyzed"
        },
        "moduleInstance": {
          "type": "string",
          "index": "not_analyzed"
        },
        "moduleCall": {
          "type": "string",
          "index": "not_analyzed"
        },
        "run": {
          "type": "integer"
        },
        "lumi": {
          "type": "integer"
        },
        "eventInPrc": {
          "type": "long"
        },
        "message": {
          "type": "string"
        },
        "lexicalId": {
          "type": "string",
          "index": "not_analyzed"
        },
        "msgtime": {
          "type": "date",
          "format": "YYYY-mm-dd HH:mm:ss"
        },
        "msgtimezone": {
          "type": "string",
          "index": "not_analyzed"
        },
        "date" : {
          "type": "date"
        }
      }
    }
  }
}


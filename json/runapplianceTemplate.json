{
  "template": "run*appliance*",

  "settings": {
    "analysis": {
      "analyzer": {
        "prefix-test-analyzer": {
          "type": "custom",
          "tokenizer": "prefix-test-tokenizer"
        }
      },
      "tokenizer": {
        "prefix-test-tokenizer": {
          "type": "path_hierarchy",
          "delimiter": "_"
        }
      }
    },
    "index": {
      "number_of_shards": 2,
      "number_of_replicas": 1
    }
  },
  "mappings": {
    "prc-i-state": {
      "properties": {
        "macro": {
          "type": "integer"
        },
        "mini": {
          "type": "integer"
        },
        "micro": {
          "type": "integer"
        },
        "tp": {
          "type": "double"
        },
        "lead": {
          "type": "double"
        },
        "nfiles": {
          "type": "integer"
        },
        "lockwaitUs": {
          "type": "double"
        },
        "lockcount": {
          "type": "integer"
        },
        "fm_date": {
          "type": "date",
           "format": "dateOptionalTime"
        },
        "source": {
          "type": "string",
          "index":"not_analyzed"
        },
        "mclass" : {
          "type": "string",
          "index":"not_analyzed"
        }
      },
      "_timestamp": {
        "enabled": true,
        "store": true,
        "path": "fm_date"
      },
      "_ttl": {
        "enabled": true,
        "default": 300000
      }
    },
    "prc-s-state": {
      "properties": {
        "macro": {
          "type": "integer"
        },
        "mini": {
          "properties":{
            "key":{"type":"integer","index":"no"},
            "value":{"type":"integer","index":"no"}
          }
        },
        "micro": {
          "properties":{
            "key":{"type":"integer","index":"no"},
            "value":{"type":"integer","index":"no"}
          }
        },
        "inputStats" : {
          "properties" : {
            "tp": {
              "type": "double"
            },
            "lead": {
              "type": "double"
            },
            "nfiles": {
              "type": "integer"
            },
            "lockwaitUs": {
              "type": "double"
            },
            "lockcount": {
              "type": "integer"
            }
          }
        },
        "ls": {
          "type": "integer"
        },
        "fm_date": {
          "type": "date",
          "format": "dateOptionalTime"
        },
        "source" : {
          "type":"string",
          "index":"not_analyzed"
        },
        "mclass" : {
          "type": "string",
          "index":"not_analyzed"
        }
      },
      "_timestamp": {
        "enabled": true,
        "store": true,
        "path" : "fm_date"
      }
    },
    "prc-out": {
      "properties": {
        "data": {
          "properties": {
            "in": {
              "type": "integer"
            },
            "out": {
              "type": "integer"
            },
            "file": {
              "type": "string",
              "index": "not_analyzed"
            }
          }
        },
        "ls": {
          "type": "integer",
          "store": true
        },
        "stream": {
          "type": "string",
          "index": "not_analyzed"
        },
        "source": {
          "type": "string",
          "index": "not_analyzed"
        }
      },
      "_timestamp": {
        "enabled": true,
        "store": true
      }
    },
    "prc-in": {
      "properties": {
        "data": {
          "properties": {
            "out": {
              "type": "integer"
            },
            "size": {
              "type" : "long"
            }
          }
        },
        "ls": {
          "type": "integer",
          "store": true
        },
        "index": {
          "type": "integer"
        },
        "process": {
          "type":"integer"
        },
        "source": {
          "type": "string",
          "index":"not_analyzed"
        },
        "fm_date": {
          "type": "date",
          "format": "dateOptionalTime"
        }
      },
      "_timestamp": {
        "enabled": true,
        "store": true,
        "path": "fm_date"
      }
    },
    "fu-out": {
      "properties": {
        "data": {
          "properties": {
            "in": {
              "type": "integer"
            },
            "out": {
              "type": "integer"
            },
            "errorEvents": {
              "type": "integer"
            },
            "hltErrorEvents": {
              "type": "integer"
            },
            "returnCodeMask": {
              "type": "string",
              "index": "not_analyzed"
            },
            "fileSize": {
              "type": "long"
            },
            "fileAdler32": {
              "type": "long"
            },
            "TransferDestination": {
              "type": "string",
              "index": "not_analyzed"
            },
            "InputFiles": {
                  "type": "string",
                  "index": "not_analyzed"
            }
          }
        },
        "ls": {
          "type": "integer"
        },
        "stream": {
          "type": "string",
          "index": "not_analyzed"
        },
        "host": {
          "type":"string",
          "index": "not_analyzed"
        },
        "source": {
          "type": "string",
          "index_analyzer": "prefix-test-analyzer",
          "search_analyzer": "keyword"
        },
        "fm_date": {
          "type": "date",
          "format": "dateOptionalTime"
        }
      },
      "_timestamp": {
        "enabled": true,
        "store": true,
        "path": "fm_date"
      }
    },
    "fu-complete": {
      "properties": {
        "host": {
          "type": "string",
          "index": "not_analyzed"
        },
        "fm_date": {
          "type": "date",
          "format": "dateOptionalTime"
        }
      },
      "_timestamp": {
        "enabled": true,
        "store": true,
        "path": "fm_date"
      }
    },
    "qstatus": {
      "properties": {
        "numQueuedLS": {
          "type": "integer"
        },
        "maxQueuedLS": {
          "type": "integer"
        },
        "numReadFromQueueLS": {
          "type": "integer"
        },
        "maxClosedLS": {
          "type": "integer"
        },
        "numReadOpenLS": {
          "type": "integer"
        },
        "CMSSWMaxLS": {
          "type": "integer"
        },
        "fm_date": {
          "type": "date",
          "format": "dateOptionalTime"
        },
        "host": {
          "type": "string",
          "index":"not_analyzed"
        }
      },
      "_timestamp": {
        "enabled": true,
        "store": true,
        "path": "fm_date"
      }
    },
    "cmsswlog": {
      "_timestamp": {
        "enabled": true,
        "store": true
      },
      "_ttl": {
        "enabled": true,
        "default": 2592000000
      },
      "properties": {
        "host": {
          "type": "string",
          "index": "not_analyzed"
        },
        "pid": {
          "type": "integer"
        },
        "type": {
          "type": "string",
          "index": "not_analyzed"
        },
        "severity": {
          "type": "string",
          "index": "not_analyzed"
        },
        "severityVal": {
          "type": "integer"
        },
        "category": {
          "type": "string",
          "index": "not_analyzed"
        },
        "fwkState": {
          "type": "string",
          "index": "not_analyzed"
        },
        "module": {
          "type": "string",
          "index": "not_analyzed"
        },
        "moduleInstance": {
          "type": "string",
          "index": "not_analyzed"
        },
        "moduleCall": {
          "type": "string",
          "index": "not_analyzed"
        },
        "run": {
          "type": "integer"
        },
        "lumi": {
          "type": "integer"
        },
        "eventInPrc": {
          "type": "long"
        },
        "message": {
          "type": "string"
        },
        "lexicalId": {
          "type": "string",
          "index": "not_analyzed"
        },
        "msgtime": {
          "type": "date",
          "format": "YYYY-mm-dd HH:mm:ss"
        },
        "msgtimezone": {
          "type": "string",
          "index": "not_analyzed"
        }
      }
    }
  }
}

